{
  "Serilog": {
    "MinimunLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Inofrmation"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"

        }
      },
      {
        "Name": "Seq",
        "Args":  {"serverUrl": "http://localhost:5341"}
      }
    ]
  },
  "AllowedHosts": "*"
}